<h1>New Project</h1>
<%= form_for @project do |f| -%>
<% if @project.errors.any? -%>
  <div id="errorExplanation">
    <h2><%= t('errors') %></h2>
    <ul>
    <% @project.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end -%>
<p>
	<%= f.label :team_id %>
	<%= f.collection_select :team_id, Team.all, :id, :name %>
</p>
<p>
	<%= f.label :backlog_id %>
	<%= f.collection_select :backlog_id, Backlog.all, :id, :name %>
</p>
<p><%= f.submit "Create", :disable_with => 'Creating...' %>
<% end -%>